{
    "presets": [
      ["@babel/env", {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": "> 5.0%, not dead"
      }],
      "@babel/typescript" 
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", {"decoratorsBeforeExport": true}],
      ["@babel/proposal-class-properties"],
      "@babel/proposal-object-rest-spread"
    ]
  }